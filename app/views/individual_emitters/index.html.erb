<h1> <%= @individual.name + "'s Emitters" %> </h1>
<% @emitters.each do |emitter| %>
<p><%= emitter.appliance%></p>
<p><%= emitter.co2e_per_hour%></p>
<p><%= emitter.hours_per_day%></p>
<p><%= emitter.use%></p>
<% end %>
<p><%= link_to "Add an emitter for #{@individual.name}", "/individuals/#{@individual.id}/emitters/new" %></p>
<p><%= link_to "All Individuals", "/individuals" %></p>
<p><%= link_to "All Emitters", "/emitters" %></p>